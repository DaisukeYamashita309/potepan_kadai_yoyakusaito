<h2>プロフィール</h2>
<p>アイコン画像</p>
 <%= link_to "ログアウト", destroy_user_session_path, method: :delete %><br>
 
 <%= link_to "アカウント", users_account_path %>
 <%= link_to "プロフィール", users_index_path %>
 
 <table>
  <tbody>
   <%= form_for @users, url: users_index_path do |f| %>
   <tr>
       <th><%= f.label :photo ,"アイコン画像" %></th>
      <%= file_field_tag 'attachment'%>
   </tr>
     <tr>
      <th><%= f.label :introduction ,"自己紹介" %></th>
      <td><%= f.text_area :introduction ,placeholder:"自己紹介を入力"%></td>
</tr>
<tr>
    <th><%= f.label :name ,"名前" %></th>
    <td><%= f.text_field :name%></td>
</tr>
   </tbody>
  </table>
  
  <ul>
      <li><%= f.submit "更新"%></li>
    <% end %>
 